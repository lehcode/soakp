{
  "name": "@lehcode/openai-browser",
  "version": "1.0.1",
  "description": "A Javascript browser library for the OpenAI API which securely handles your key.",
  "repository": {
    "type": "git",
    "url": "git@github.com:lehcode/openai-browser.git"
  },
  "keywords": [
    "openai",
    "key",
    "browser",
    "gpt",
    "gpt3",
    "gpt-3",
    "gpt4",
    "gpt-4"
  ],
  "author": "Antony Repin",
  "license": "MIT",
  "main": "./dist/browser.js",
  "types": "./dist/browser.d.ts",
  "scripts": {
    "build": "tsc --outDir ./dist/ && sh -c './to-mjs.sh'",
    "lint": "eslint 'src/**/*.{ts}' --fix --quiet --",
    "dev": "npm run build && NODE_OPTIONS=\"--no-warnings --inspect --experimental-modules\" ts-node dist/keyserver.mjs",
    "start": "npm run build && NODE_OPTIONS=\"--no-warnings --experimental-modules\" ts-node dist/keyserver.mjs"
  },
  "type": "module",
  "dependencies": {
    "axios": "^0.26.0",
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "express-basic-auth": "^1.2.1",
    "form-data": "^4.0.0",
    "jsonwebtoken": "^9.0.0",
    "openai": "^3.2.1",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@tsconfig/node-lts": "^18.12.2",
    "@tsconfig/recommended": "^1.0.2",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^12.20.55",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "browserslist": "^4.21.5",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "multer": "^1.4.5-lts.1",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.2",
    "typescript": "^5.0.4"
  },
  "browserslist": [
    "defaults and supports es6-module",
    "last 2 versions"
  ]
}
