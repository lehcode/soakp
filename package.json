    "@typescript-eslint/parser": "^5.59.8",
    "babel-jest": "^29.5.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "browserslist": "^4.21.5",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.2",
    "typescript": "^5.1.3"
  }
