{
  "name": "@lehcode/openai-proxy",
  "version": "1.0.1",
  "description": "A key storage and proxy for OpenAI.",
  "repository": {
    "type": "git",
    "url": "git@github.com:lehcode/openai-proxy.git"
  },
  "keywords": [
    "openai",
    "proxy",
    "gpt",
    "gpt3",
    "gpt-3",
    "gpt4",
    "gpt-4"
  ],
  "module": "node",
  "author": "Antony Repin",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist/*.* && npx tsc --outDir ./dist/",
    "lint": "eslint 'src/**/*.{ts}' --fix --quiet",
    "dev": "npm run build ./dist/ && NODE_OPTIONS=\"--inspect --trace-warnings\" ts-node --esm index.ts",
    "dev-js": "npm run build && NODE_OPTIONS=\"--inspect --trace-warnings\" ts-node dist/index.js",
    "start": "npm run build"
  },
  "dependencies": {
    "axios": "^0.26.0",
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "express-basic-auth": "^1.2.1",
    "form-data": "^4.0.0",
    "jsonwebtoken": "^9.0.0",
    "node-persist": "^3.1.3",
    "openai": "^3.2.1",
    "sqlite3": "^5.1.6",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@tsconfig/node-lts": "^18.12.2",
    "@tsconfig/recommended": "^1.0.2",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^12.20.55",
    "@types/node-persist": "^3.1.3",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "browserslist": "^4.21.5",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.2"
  }
}
