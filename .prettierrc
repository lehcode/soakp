{
  "singleQuote": false,
  "semi": true,
  "tabWidth": 2,
  "useTabs": false,
  "trailingComma": "none",
  "printWidth": 120,
  "overrides": [
    {
      "files": ["*.ts"],
      "options": {
        "parser": "typescript",
        "singleQuote": true
      }
    },
    {
      "files": ["*.json"],
      "options": {
        "singleQuote": false
      }
    }
  ]
}
